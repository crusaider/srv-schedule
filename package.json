{
  "name": "@crusaider/srv-sechedule",
  "version": "1.0.0",
  "description": "Get fetching schedule for SRV from https://www.srvatervinning.se/avfallshamtning/nar-hamtar-vi-ditt-avfall",
  "author": "Jonas Andreasson",
  "license": "MIT",
  "keywords": [
    "SRV",
    "Schedule"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/crusaider/srv-schedule.git"
  },
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest ./src",
    "test:integration": "jest ./tests",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint ./src ./tests",
    "format": "prettier --write .",
    "prepare": "husky",
    "prepublishOnly": "npm test && npm run build"
  },
  "lint-staged": {
    "*.ts": "eslint",
    "*.(ts|js|mjs|json)": "prettier --write"
  },
  "dependencies": {
    "cross-fetch": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/jest": "^29.5.14",
    "eslint": "^9.19.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  }
}
